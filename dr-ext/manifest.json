{
    "manifest_version": 3,
    "name": "Dungeon Report Viewer",
    "description": "Enables Quick Access to a players Dungeon Report on Bungie LFG.",
    "version": "0.1",
    
    "permissions": ["contextMenus","tabs"],
    "icons": {
        "16": "/images/dr16.png",
        "48": "/images/dr48.png",
        "128": "/images/dr128.png"
    },
    "background": { 
        "service_worker": "background.js"
    }
}